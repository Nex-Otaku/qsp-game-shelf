{
    "name": "nex-otaku/yii2-app-ganimede",
    "description": "Yii 2 Ganimede Project Template",
    "keywords": ["yii2", "framework", "ganimede", "project template"],
    "homepage": "https://github.com/Nex-Otaku/yii2-app-ganimede",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/yii2/issues?state=open",
        "forum": "http://www.yiiframework.com/forum/",
        "wiki": "https://github.com/Nex-Otaku/yii2-app-ganimede/wiki",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/Nex-Otaku/yii2-app-ganimede"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": ">=2.0.6",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "dektrium/yii2-user": "^0.9.6",
        "dmstr/yii2-adminlte-asset": "^2.3",
        "nex-otaku/yii2-toolkit": "dev-master",
        "nex-otaku/yii2-uuid-tools": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "insolita/yii2-migration-generator": "^1.0"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        },
        "asset-vcs-driver-options": {
            "github-no-api": true
        },
        "asset-pattern-skip-version": "(-build|-patch)"
    },
    "repositories":
    [
        {
            "type": "vcs",
            "url": "https://github.com/nex-otaku/yii2-p2y2-things"
        },
        {
            "type": "vcs",
            "url": "file://D:/Dev/repos/yii2-uuid-tools"
        }
    ]
}
